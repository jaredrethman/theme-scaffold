{
  "name": "tenup-scaffold",
  "version": "1.0.0",
  "description": "Project Description",
  "homepage": "https://project-domain.tld",
  "repository": {
    "type": "git",
    "url": "https://project-git-repo.tld"
  },
  "author": {
    "name": "10up",
    "email": "info@10up.com",
    "url": "https://10up.com",
    "role": "developer"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development  --disableHostCheck=true --config webpack/webpack.dev.js",
    "dev:s": "cross-env NODE_ENV=development webpack-dev-server --mode development  --disableHostCheck=true --config webpack/webpack.dev.js --https --cert ./ssl/localhost.cert --key ./ssl/localhost.key",
    "build": "cross-env NODE_ENV=build webpack --config webpack/webpack.build.js",
    "prod": "cross-env NODE_ENV=production webpack --config webpack/webpack.prod.js",
    "watch": "cross-env NODE_ENV=watch --hot=false webpack --watch --config webpack/webpack.dev.js",
    "start": "composer install --ignore-platform-reqs && npm i && npm run build",
    "build-release": "npm install && composer install --no-dev -o && npm run build",
    "lint-release": "npm install && composer install && npm run lint",
    "lint-css": "stylelint assets/css",
    "lint-js": "eslint assets/js",
    "lint-php": "composer run lint",
    "lint": "npm run lint-css && npm run lint-js && npm run lint-php",
    "test:a11y": "node tests/accessibility/compliance/pa11y.js",
    "test:visual-init": "pushd tests/visual/ && backstop init && rm backstop.json && popd",
    "test:visual-reference": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js reference --config=index.js && popd",
    "test:visual": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js test --config=index.js && popd",
    "test:visual-approve": "pushd tests/visual/ && ../../node_modules/backstopjs/cli/index.js approve --config=index.js && popd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "stylelint"
    ],
    "*.js": [
      "eslint"
    ],
    "*.php": [
      "./vendor/bin/phpcs --extensions=php --warning-severity=8 -s"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@10up/eslint-config": "^1.0.9",
    "@10up/stylelint-config": "^1.0.7",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@hot-loader/react-dom": "^16.9.0",
    "@wordpress/babel-preset-default": "^4.6.0",
    "@wordpress/element": "^2.8.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "chalk": "^2.4.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.1",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^4.2.0",
    "glob-all": "^3.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "pa11y": "^5.2.0",
    "postcss-editor-styles": "^0.1.5",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "purgecss-webpack-plugin": "^1.6.0",
    "react-hot-loader": "^4.12.13",
    "style-loader": "^1.0.0",
    "stylelint": "^9.10.1",
    "stylelint-config-wordpress": "^14.0.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-order": "^3.1.1",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser": "^4.3.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.1",
    "webpack-fix-style-only-entries": "^0.4.0",
    "webpack-merge": "^4.2.2",
    "webpackbar": "^4.0.0"
  },
  "testing": {
    "urls": {
      "homepage": "http://tenup-scaffold.test",
      "article": "http://tenup-scaffold.test/hello-world",
      "search-results": "http://tenup-scaffold.test/?s=hello"
    },
